{
	"name": "secure_tenet",
	"version": "1.0.0",
	"private": "true",
	"description": "Herramienta de analisis de seguridad de datos.",
	"keywords": [
		"datos"
	],
	"author": "Luis Rizzo.",
	"type": "module",
	"scripts": {
		"dev": "astro dev",
		"start": "astro dev",
		"build": "astro check && astro build",
		"preview": "astro preview",
		"astro": "astro",
		"lint": "eslint .",
		"lint:css": "stylelint \"**/*.{astro,css}\"",
		"format": "pnpm exec prettier . --write",
		"prepare": "husky",
		"clear": "shx rm -rf dist .astro src/astro_tmp_pages_*",
		"upgrade": "NPM_CHECK_INSTALLER=pnpm npm-check -u",
		"upgrade:astro": "pnpm dlx @astrojs/upgrade"
	},
	"dependencies": {
		"@astrojs/check": "^0.7.0",
		"@astrojs/sitemap": "^3.1.6",
		"@astrojs/tailwind": "^5.1.0",
		"@astrojs/vercel": "^8.1.3",
		"@faker-js/faker": "^8.4.1",
		"apexcharts": "^3.49.2",
		"astro": "^5.5.6",
		"canvg": "^4.0.3",
		"dompurify": "^3.2.4",
		"flowbite": "^2.4.1",
		"flowbite-typography": "^1.0.3",
		"html2canvas": "^1.4.1",
		"tailwind-scrollbar": "^3.1.0",
		"tailwindcss": "^3.4.4"
	},
	"devDependencies": {
		"@commitlint/cli": "^19.3.0",
		"@commitlint/config-conventional": "^19.2.2",
		"@tailwindcss/typography": "^0.5.9",
		"@trivago/prettier-plugin-sort-imports": "^4.2.0",
		"@typescript-eslint/parser": "^7.15.0",
		"eslint": "^9.6.0",
		"eslint-plugin-astro": "^1.2.2",
		"husky": "^9.0.11",
		"lint-staged": "^15.2.7",
		"postcss-html": "^1.7.0",
		"prettier": "^3.3.2",
		"prettier-config-standard": "^7.0.0",
		"prettier-plugin-astro": "^0.14.0",
		"prettier-plugin-tailwindcss": "^0.5.4",
		"shx": "^0.3.4",
		"standard-version": "^9.5.0",
		"stylelint": "^16.6.1",
		"stylelint-config-html": "^1.1.0",
		"typescript": "^5.5.3"
	}
}
